{
  "name": "express-ts-project",
  "version": "1.0.0",
  "description": "A simple Express project using TypeScript",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r dotenv/config dist/app.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node -r dotenv/config' src/app.ts",
    "dev:debug": "nodemon --watch 'src/**/*.ts' --exec 'ts-node -r dotenv/config' src/app.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.14",
    "@types/node": "^16.11.7",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "author": "",
  "license": "ISC"
}
